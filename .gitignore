# Laravel .gitignore

# Dependencies
/node_modules
/vendor

# Laravel specific
/bootstrap/cache/*.php
/storage/app/*
!/storage/app/.gitkeep
!/storage/app/public
!/storage/app/public/uploads
!/storage/app/public/uploads/products
/storage/framework/cache/*
!/storage/framework/cache/.gitkeep
/storage/framework/sessions/*
!/storage/framework/sessions/.gitkeep
/storage/framework/testing/*
!/storage/framework/testing/.gitkeep
/storage/framework/views/*
!/storage/framework/views/.gitkeep
/storage/logs/*
!/storage/logs/.gitkeep

# Environment files
.env
.env.backup
.env.production
.phpunit.result.cache

# IDE files
.vscode/
.idea/
*.swp
*.swo
*~

# OS generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# Build files
/public/hot
/public/storage
/public/build
/public/mix-manifest.json

# Logs
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Runtime data
pids
*.pid
*.seed
*.pid.lock

# Coverage directory used by tools like istanbul
coverage/

# nyc test coverage
.nyc_output

# Dependency directories
jspm_packages/

# Optional npm cache directory
.npm

# Optional REPL history
.node_repl_history

# Output of 'npm pack'
*.tgz

# Yarn Integrity file
.yarn-integrity

# dotenv environment variables file
.env.test

# parcel-bundler cache (https://parceljs.org/)
.cache
.parcel-cache

# next.js build output
.next

# nuxt.js build output
.nuxt

# vuepress build output
.vuepress/dist

# Serverless directories
.serverless

# FuseBox cache
.fusebox/

# DynamoDB Local files
.dynamodb/

# Temporary folders
tmp/
temp/

# Laravel Mix
/public/css
/public/js

# Backup files
*.bak
*.backup

# Test files
/tests/coverage/

# Composer
composer.phar
/vendor/

# Laravel Telescope (if used)
/storage/telescope/

# Laravel Horizon (if used)
/storage/horizon/

# Backup databases
*.sql
*.sqlite

# Custom uploads (keep structure but ignore content)
/public/uploads/*
!/public/uploads/.gitkeep

# Cache files
/bootstrap/cache/*
!/bootstrap/cache/.gitkeep

# Session files
/storage/framework/sessions/*
!/storage/framework/sessions/.gitkeep

# Compiled views
/storage/framework/views/*
!/storage/framework/views/.gitkeep